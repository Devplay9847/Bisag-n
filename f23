SELECT emp_name FROM tb_employee_dtl 
WHERE emp_name LIKE 'a%'
OR emp_name LIKE 'm%'
OR emp_name LIKE 'k%'
OR emp_name LIKE 'p%'
OR emp_name LIKE '%n'
OR emp_name LIKE '%l'
OR emp_name LIKE '%h'


